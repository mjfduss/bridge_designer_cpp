# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore the RDIS test database
/db/*.rdb

# Ignore cached assets.
/public/standings/**
/public/admin/**

# Don't vc the mail account info
/config/initializers/mail.rb

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# For now, we need to restrict distribution of the scramble key to serious developers.
# If you want it, just write us to explain why.
/vendor/gems/WPBDC/ext/WPBDC/rc4_key.c

# WPBD gem will be installed from rubygems.org
*.gem

# Ignore auto-generated files but track manual sources.
/doc/**
!manual.tex
!manual.pdf

# Ignore other unneeded files.
*.swp
*.bat
*.cmd
*.o
*.bundle
*~
\#*#
.project
.DS_Store
.idea

# We sometimes use foo files for experiments
foo.*
