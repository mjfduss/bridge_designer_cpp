!!! 5
%html
  %head
    %title Engineering Encounters Bridge Design Contest
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    /[if lt IE 9]
      = javascript_include_tag 'html5fix'
  %body
    - if session.has_key?(:admin_id)
      %h1.trouble *** Admin Session: #{Schedule::DESCRIPTIONS[@schedule_state]} ***
    - if content_for?(:dialog)
      .dialog.stdwidth
        .dialoghead
          Engineering Encounters Bridge Design Contest Registration
        .dialogbody
          = yield :dialog
    = yield
    - if Rails.env.development?
      = debug(session)
      = debug(params)
