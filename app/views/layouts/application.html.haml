!!! 5
%html
  %head
    %title West Point Bridge Design Contest
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    /[if lt IE 9]
      = javascript_include_tag 'html5fix'
  %body
    - if content_for?(:dialog)
      .dialog.stdwidth
        .dialoghead
          West Point Bridge Design Contest Registration
        .dialogbody
          = yield :dialog
    = yield
    = debug(session) if Rails.env.development?
    = debug(params) if Rails.env.development?
